---
layout: null
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>Blog | Nolan Conaway</title>
	<link href="{{ site.url }}/blog/atom.xml" rel="self"/>
	<link href="{{ site.url }}/blog"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>{{ site.url }}/blog</id>
	<author>
		<name>Nolan Conaway</name>
		<email>nolanbconaway@gmail.com</email>
	</author>

	{% for post in site.posts %}
		<entry>
			<title>{{post.season }}: {{ post.title }}</title>
			<link href="{{ site.url }}{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>{{ site.url }}{{ post.id }}</id>
			<content type="html">
				{{ post.description | xml_escape }}
				Keywords: {{post.keywords}} 
			</content>
		</entry>
	{% endfor %}

</feed>